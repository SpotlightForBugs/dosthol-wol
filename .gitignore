# dosthol .gitignore

# Temporary files
*.tmp
*.temp
/tmp/
/tmp/dosthol*

# Log files
*.log
logs/

# Backup files
*.bak
*.backup
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Editor files
.vscode/
.idea/
*.swp
*.swo
*~

# Package files
*.deb
*.rpm
*.tar.gz
*.zip

# Configuration files with sensitive data
config.local
.env

# Test files
test_*
*_test

# Documentation build
docs/_build/
site/

